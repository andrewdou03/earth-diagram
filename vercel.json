{
  "headers": [
    {
      "source": "/(.*)",
      "has": [{ "type": "query", "key": "embed", "value": "1" }],
      "headers": [
        {
          "key": "Content-Security-Policy",
          "value": "frame-ancestors 'self' http://localhost:3000 https://your-portfolio-domain.com https://www.your-portfolio-domain.com"
        }
      ]
    },
    {
      "source": "/(.*)",
      "headers": [
        { "key": "Content-Security-Policy", "value": "frame-ancestors 'self'" }
      ]
    }
  ]
}
